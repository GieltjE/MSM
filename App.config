    <ThrowUnobservedTaskExceptions enabled="true"/>
  </runtime>
  <userSettings>
    <MSM.Properties.Settings>
      <setting name="location" serializeAs="String">
        <value>50, 50</value>
      </setting>
      <setting name="size" serializeAs="String">
        <value>800, 500</value>
      </setting>
      <setting name="theme" serializeAs="String">
        <value>dark</value>
      </setting>
    </MSM.Properties.Settings>
  </userSettings>
</configuration>
